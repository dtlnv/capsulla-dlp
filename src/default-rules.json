{
    "clipboardRules": [
        {
            "mask": "[EMAIL]",
            "pattern": "\\b[\\w.-]+@[\\w.-]+\\.\\w{2,4}\\b",
            "react": "warn"
        },
        {
            "mask": "[SSN]",
            "pattern": "\\b\\d{3}-\\d{2}-\\d{4}\\b",
            "react": "warn"
        },
        {
            "mask": "[SSN]",
            "pattern": "\\b\\d{3} \\d{2}-\\d{4}\\b",
            "react": "warn"
        },
        {
            "mask": "[SSN]",
            "pattern": "\\b\\d{2}-\\d{2}-\\d{4}\\b",
            "react": "warn"
        },
        {
            "mask": "[CARD]",
            "pattern": "\\b(?:\\d[ -]*?){13,16}\\b",
            "react": "warn"
        },
        {
            "mask": "[CARD]",
            "pattern": "\\b\\d{4}-\\d{4}-\\d{4}-\\d{4}\\b",
            "react": "warn"
        },
        {
            "mask": "[CARD]",
            "pattern": "\\b\\d{16}\\b",
            "react": "warn"
        },
        {
            "mask": "[CARD]",
            "pattern": "\\b\\d{4} \\d{4} \\d{4} \\d{4}\\b",
            "react": "warn"
        },
        {
            "mask": "[CARD]",
            "pattern": "\\b\\d{3}-\\d{3}-\\d{3}-\\d{4}\\b",
            "react": "warn"
        },
        {
            "mask": "[CARD]",
            "pattern": "\\b\\d{3}\\.\\d{3}\\.\\d{3}\\.\\d{4}\\b",
            "react": "warn"
        },
        {
            "mask": "[PHONE]",
            "pattern": "\\b\\d{10,11}\\b",
            "react": "warn"
        },
        {
            "mask": "[PHONE]",
            "pattern": "\\b\\d{3}-\\d{3}-\\d{4}\\b",
            "react": "warn"
        },
        {
            "mask": "[PHONE]",
            "pattern": "\\b\\d{3}\\.\\d{3}\\.\\d{4}\\b",
            "react": "warn"
        },
        {
            "mask": "[PHONE]",
            "pattern": "\\b\\d{3} \\d{3} \\d{4}\\b",
            "react": "warn"
        }
